<Window x:Class="MarketWPF.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarketWPF"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Name="UsernameTextBox" Margin="10"/>
            <PasswordBox Name="PasswordBox"  Margin="10"/>
            <PasswordBox Name="ConfirmPasswordBox"   Margin="10"/>
            <Button Name="RegisterButton" Content="Register" Margin="10" Click="RegisterButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
